:set nocompatible
:set number
:set tabstop=2
:set expandtab
:set shiftwidth=2
:set backspace=2
:set term=xterm
:set autowrite
map <F5> :!clear & mix test<CR>
inoremap jj <ESC>
let mapleader=","
inoremap <up> <nop>
noremap <up> <nop>
inoremap <down> <nop>
noremap <down> <nop>
inoremap <left> <nop>
noremap <left> <nop>
inoremap <right> <nop>
noremap <right> <nop>
:set hlsearch
